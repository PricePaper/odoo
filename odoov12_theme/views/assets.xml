<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="custom assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/odoov12_theme/static/src/css/odoov12_theme.css" />
        </xpath>
    </template>


    <template id="odoov12_theme_assets_primary_variables" inherit_id="web_enterprise._assets_primary_variables">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/odoov12_theme/static/src/scss/primary_variables.scss" />
        </xpath>
    </template>


    <template id="odoov12_theme_assets_secondary_variables" inherit_id="web_enterprise._assets_secondary_variables">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/odoov12_theme/static/src/scss/secondary_variables.scss" />
        </xpath>
    </template>

</odoo>
