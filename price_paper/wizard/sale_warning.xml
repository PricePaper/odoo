<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
      <record id="view_sale_warning_wizard" model="ir.ui.view">
          <field name="name">sale.warning.wizard</field>
          <field name="model">sale.warning.wizard</field>
          <field name="arch" type="xml">
              <form string="Warning">
                  <sheet>
                      <group>
                          <group>
                              <field name="warning_message" readonly='1'/>
                          </group>
                      </group>
                  </sheet>
                  <footer>
                      <button string="Close" type="object" name="close_window"/>
                  </footer>
              </form>
          </field>
      </record>
  </data>
</odoo>
